{
  "name": "rndm-react-xp",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "RNDM",
  "license": "Apache-2.0",
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "bundle:ios": "node_modules/.bin/react-native bundle --entry-file='src/index.native.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "start": "node node_modules/.bin/serve -s build",
    "postinstall": "node _scripts/postInstall/index.js; npm run install-pods; npm run install-fonts;",
    "packager": "node node_modules/react-native/local-cli/cli.js start -- -reset-cache",
    "ios": "./node_modules/.bin/react-native run-ios --scheme rndm-ios",
    "tvos": "xcrun xcodebuild -workspace ios/rndm.xcworkspace -scheme rndm-tvos -configuration Debug -derivedDataPath './ios/build' -destination 'platform=tvOS Simulator,name=Apple TV 4K,OS=12.1' | xcpretty && ./node_modules/.bin/ios-sim launch ios/build/Build/Products/Debug-appletvsimulator/rndm-tvos.app --devicetypeid 'Apple-TV-4K-4K, tvOS 12.1'",
    "android": "./node_modules/.bin/react-native run-android",
    "windows": "react-native run-windows",
    "web": "react-app-rewired start",
    "test": "mocha \"./__tests__/unit/**/*.spec.js\" --opts ./__tests__/unit/_setup/mocha.opts",
    "cover": "nyc --check-coverage npm run test",
    "lint": "node_modules/.bin/eslint src",
    "install-pods": "cd ios; pod install",
    "install-fonts": "cp -R node_modules/react-native-vector-icons/Fonts/. android/app/src/main/assets/fonts;",
    "loc": "cloc src ios/ninenow android/app/src __tests__ --exclude-ext=json,xml,stub.js",
    "loc-src": "cloc src ios/ninenow android/app/src --exclude-ext=json,xml,stub.js",
    "loc-src-js": "cloc src --exclude-ext=json,xml,stub.js",
    "loc-tests": "cloc __tests__ --exclude-ext=json,xml,stub.js",
    "loc-tests-unit": "cloc __tests__/unit --exclude-ext=json,xml,stub.js",
    "loc-tests-ui": "cloc __tests__/ui --exclude-ext=json,xml,stub.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "clean-deep": "3.4.0",
    "compare-versions": "3.6.0",
    "crypto-js": "4.0.0",
    "detect-browser": "5.0.0",
    "firebase": "8.6.1",
    "react": "17.0.2",
    "react-art": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.64.1",
    "react-native-device-info": "^8.1.3",
    "react-native-elements": "3.4.1",
    "react-native-vector-icons": "8.1.0",
    "react-native-web": "0.16.3",
    "react-native-windows": "1.0.0",
    "react-navigation": "4.4.4",
    "react-navigation-redux-helpers": "4.0.1",
    "react-redux": "7.2.4",
    "react-scripts": "1.1.4",
    "redux": "4.1.",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^^7.13.16",
    "@babel/plugin-proposal-class-properties": "^^7.13.16",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^^7.13.16",
    "@babel/plugin-proposal-object-rest-spread": "^^7.13.16",
    "@babel/plugin-transform-runtime": "^^7.13.16",
    "@babel/preset-env": "^^7.13.16",
    "@babel/preset-flow": "^^7.13.16",
    "@babel/preset-react": "^^7.13.16",
    "@rndm/test-gen": "0.2.0",
    "babel-node-modules": "0.0.1",
    "babel-plugin-istanbul": "6.0.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "colors": "1.4.0",
    "customize-cra": "^0.9.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-standard": "8.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-standard": "4.0.1",
    "ios-sim": "9.0.0",
    "jest-serializer": "25.2.6",
    "metro-react-native-babel-preset": "^0.66.0",
    "mocha": "7.1.1",
    "nyc": "15.0.1",
    "proxyquire": "2.1.3",
    "react-app-rewire-babel-loader": "^0.1.1",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewired": "2.0.0",
    "react-dev-utils": "^11.0.4",
    "rnpm-plugin-windows": "0.6.1",
    "serve": "11.3.0",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0"
  },
  "nyc": {
    "lines": 70,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.flow.js",
      "src/web",
      "src/index.js",
      "src/index.native.js",
      "flow-typed"
    ],
    "require": [
      "@babel/register"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
